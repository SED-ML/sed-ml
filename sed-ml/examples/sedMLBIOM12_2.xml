<?xml version="1.0" encoding="utf-8"?>
<sedML xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.biomodels.net/sed-ml/../documents/schema/sedml-tmp.xsd" xmlns:math="http://www.w3.org/1998/Math/MathML" xmlns="http://www.biomodels.net/sed-ml" version="0.1">
  <notes> Showing the switch in behaviour from oscillation to stable depending on the 
    total number of proteins per cell.
  </notes>
  <listOfSimulations>
    <notes />
    <uniformTimeCourse id="simulation1" algorithm="KiSAO:0000088" initialTime="0" outputStartTime="0" outputEndTime="1000" numberOfPoints="1000" />
  </listOfSimulations>
  <listOfModels>
    <model id="model1" name="Repressilator" type="SBML" source="urn:miriam:biomodels.db:BIOMD0000000012" />
    <model id="model2" name="Repressilator_2" type="SBML" source="model1">
      <listOfChanges>
        <changeAttribute target="/sbml:sbml/sbml:model/sbml:listOfParameters/sbml:parameter[@id='ps_a']/@value" newValue="0.013" />
        <changeAttribute target="/sbml:sbml/sbml:model/sbml:listOfParameters/sbml:parameter[@id='ps_0']/@value" newValue="1.3E-5" />
      </listOfChanges>
    </model>
  </listOfModels>
  <listOfTasks>
    <task id="task1" name="Oscillation" modelReference="model1" simulationReference="simulation1" />
    <task id="task2" name="stability" modelReference="model2" simulationReference="simulation1" />
  </listOfTasks>
  <listOfDataGenerators>
    <dataGenerator id="time" name="Time">
      <listOfVariables>
        <variable id="time" taskReference="task1" target="time" />
      </listOfVariables>
      <math xmlns="http://www.w3.org/1998/Math/MathML">
        <ci> time </ci>
      </math>
    </dataGenerator>
    <dataGenerator id="LaCI" name="LaCI repressor">
      <listOfVariables>
        <variable id="v1" taskReference="task1" target="/sbml:sbml/sbml:model/sbml:listOfSpecies/sbml:species[@id='PX']" />
      </listOfVariables>
      <math:math>
        <math:ci>v1</math:ci>
      </math:math>
    </dataGenerator>
    <dataGenerator id="TetR" name="TetR repressor">
      <listOfVariables>
        <variable id="v2" taskReference="task1" target="/sbml:sbml/sbml:model/sbml:listOfSpecies/sbml:species[@id='PY']" />
      </listOfVariables>
      <math:math>
        <math:ci>v2</math:ci>
      </math:math>
    </dataGenerator>
    <dataGenerator id="CI" name="CI repressor">
      <listOfVariables>
        <variable id="v3" taskReference="task1" target="/sbml:sbml/sbml:model/sbml:listOfSpecies/sbml:species[@id='PZ']" />
      </listOfVariables>
      <math:math>
        <math:ci>v3</math:ci>
      </math:math>
    </dataGenerator>
    <dataGenerator id="LaCI_2" name="LaCI repressor">
      <listOfVariables>
        <variable id="v4" taskReference="task2" target="/sbml:sbml/sbml:model/sbml:listOfSpecies/sbml:species[@id='PX']" />
      </listOfVariables>
      <math:math>
        <math:ci>v4</math:ci>
      </math:math>
    </dataGenerator>
    <dataGenerator id="TetR_2" name="TetR repressor">
      <listOfVariables>
        <variable id="v5" taskReference="task2" target="/sbml:sbml/sbml:model/sbml:listOfSpecies/sbml:species[@id='PY']" />
      </listOfVariables>
      <math:math>
        <math:ci>v5</math:ci>
      </math:math>
    </dataGenerator>
    <dataGenerator id="CI_2" name="CI repressor">
      <listOfVariables>
        <variable id="v6" taskReference="task2" target="/sbml:sbml/sbml:model/sbml:listOfSpecies/sbml:species[@id='PZ']" />
      </listOfVariables>
      <math:math>
        <math:ci>v6</math:ci>
      </math:math>
    </dataGenerator>
  </listOfDataGenerators>
  <listOfOutputs>
    <plot2D id="plot1" name="protein numbers per time point&gt;">
      <notes> (oscillating) </notes>
      <listOfCurves>
        <curve logX="false" logY="false" xDataReference="time" yDataReference="LaCI" />
        <curve logX="false" logY="false" xDataReference="time" yDataReference="TetR" />
        <curve logX="false" logY="false" xDataReference="time" yDataReference="CI" />
      </listOfCurves>
    </plot2D>
    <plot2D id="plot2" name="protein numbers per time point">
      <notes>asymptotic</notes>
      <listOfCurves>
        <curve logX="false" logY="false" xDataReference="time" yDataReference="LaCI_2" />
        <curve logX="false" logY="false" xDataReference="time" yDataReference="TetR_2" />
        <curve logX="false" logY="false" xDataReference="time" yDataReference="CI_2" />
      </listOfCurves>
    </plot2D>
  </listOfOutputs>
</sedML>